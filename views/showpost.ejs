<% include header %>
    <% include nav %>
        <% include slider %>

            <div id="page">
                <div id="content">
                    <div class="box">
                        <h2>show post</h2>

                        <style>
                            #mycat {
                                height: 450px;
                                width: 600px;
                            }
                            
                            #mycat1 {
                                height: 150px;
                                width: 200px;
                                float: left;
                            }
                        </style>
                        <div id="mycat">
                            <% for(i=0;i<mycat.length;i++) { %>
                                <a href="/viewshowpost/<%= mycat[i].cat_nm %>">
                                    <div id="mycat1">
                                        <center><img src="../uploads/<%= mycat[i].myimg1 %>" height="100" width="150" />
                                            <br>
                                            <b><%= mycat[i].title %></b>
                                        </center>
                                    </div>
                                </a>
                                <% } %>
                        </div>

                    </div>
                </div>
                <br class="clearfix" />
            </div>


            <% include footer %>